    for (n = 1; n<=10; n++){
        i = 1;
document.writeln("<br> Tablas del ", n , "<br>" )

        do {
            m = n * i;
            document.writeln(n, " * ", i, "=", m, "<br>");
            i++;
        } while (i < 11) 


